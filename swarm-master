( yum install -y docker
systemctl start docker
systemctl enable docker
docker run -d -v /usr/share/ca-certificates/:/etc/ssl/certs -p 4001:4001 -p 2380:2380 -p 2379:2379 --name etcd quay.io/coreos/etcd:v2.3.8 -name etcd0 -advertise-client-urls http://192.168.99.1${c}0:2379,http://192.168.99.1${c}0:4001 -listen-client-urls http://0.0.0.0:2379,http://0.0.0.0:4001 -initial-advertise-peer-urls http://192.168.99.1${c}0:2380 -listen-peer-urls http://0.0.0.0:2380 -initial-cluster-token etcd-cluster-1 -initial-cluster etcd0=http://192.168.99.1${c}0:2380 -initial-cluster-state new
docker swarm init --advertise-addr 192.168.99.1${c}0
echo End ) >&/var/log/vagrant.bootstrap &
